msgid "Cmdline"
msgstr "Кмдлін"

#: menu.sh:863
msgid "Commonly used cmdlines:\\n"
msgstr "Зазвичай використовуються смdlines:\\n"

#: menu.sh:864
msgid " * \\Z4disable_mtrr_trim=\\Zn\\n    disables kernel trim any uncacheable memory out.\\n"
msgstr " * \\Z4disable_mtrr_trim=\\Zn\\n    відключає ядро будь-якої непоганої пам'яті.\\n"

#: menu.sh:865
msgid " * \\Z4intel_idle.max_cstate=1\\Zn\\n    Set the maximum C-state depth allowed by the intel_idle driver.\\n"
msgstr " * \\Z4intel_idle.max_cstate=1\\Zn\\n    Встановити максимальну глибину C-держави, дозволену водієм intel_idle.\\n"

#: menu.sh:866
msgid " * \\Z4pcie_port_pm=off\\Zn\\n    Turn off the power management of the PCIe port.\\n"
msgstr " * \\Z4pcie_port_pm=off\\Zn\\n    Увімкніть управління живленням порту PCIe.\\n"

#: menu.sh:867
msgid " * \\Z4libata.force=noncq\\Zn\\n    Disable NCQ for all SATA ports.\\n"
msgstr " * \\Z4libata.force=noncq\\Zn\\n    Вимкнути NCQ для всіх портів SATA.\\n"

#: menu.sh:868
msgid " * \\Z4SataPortMap=??\\Zn\\n    Sata Port Map.(Not apply to DT models.)\\n"
msgstr " * \\Z4SataPortMap=??\\Zn\\n    Sata Port Map.(Не застосовується до моделей DT.)\\n"

#: menu.sh:869
msgid " * \\Z4DiskIdxMap=??\\Zn\\n    Disk Index Map, Modify disk name sequence.(Not apply to DT models.)\\n"
msgstr " * \\Z4DiskIdxMap=??\\Zn\\n    Дискова карта, Змінено послідовність назв диска.(Не застосовується до моделей DT.)\\n"

#: menu.sh:870
msgid " * \\Z4ahci_remap=4>5:5>8:12>16\\Zn\\n    Remap data port sequence.(Not apply to DT models.)\\n"
msgstr " * \\Z4ahci_remap=4>5:5>8:12>16\\Zn\\n    Послідовність порту Remap. (Не застосовується до моделей DT)\\n"

#: menu.sh:871
msgid " * \\Z4i915.enable_guc=2\\Zn\\n    Enable the GuC firmware on Intel graphics hardware.(value: 1,2 or 3)\\n"
msgstr " * \\Z4i915.enable_guc=2\\Zn\\n    Увімкніть прошивку GuC на Intel графіку.(значення: 1,2 або 3)\\n"

#: menu.sh:872
msgid " * \\Z4i915.max_vfs=7\\Zn\\n    Set the maximum number of virtual functions (VFs) that can be created for Intel graphics hardware.\\n"
msgstr " * \\Z4i915.max_vfs=7\\Zn\\n    Встановити максимальну кількість віртуальних функцій (VFs), які можуть бути створені для обладнання Intel графіки.\\n"

